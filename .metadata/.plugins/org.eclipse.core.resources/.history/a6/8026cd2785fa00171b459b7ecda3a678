package edu.fju.exam;

public class Bubble {

	public static void main(String[] args) {
		int n[] ={45, 81, 9, 72 ,4 ,6};
		for(int i=1;i<n.length-1;i++){
			for(int j=1;j<n.length-1;j++){
				if(n[j+1]>n[j]){
					int tmp = n[j];
					n[j]=n[j+1];
					n[j+1]=tmp;
					System.out.println(n[i]+" ");
				}
			}
		}
	}

}
